# NPP API Features Configuration
# 定义所有NPP API函数及其分类
# 这是NPP库的完整API功能集合

nppi_arithmetic_operations:
  description: "NPPI图像算术运算"
  functions:
    - name: "nppiAdd_8u_C1RSfs"
      description: "8位无符号单通道加法运算（饱和）"
    - name: "nppiAdd_32f_C1R"
      description: "32位浮点单通道加法运算"
    - name: "nppiSub_8u_C1RSfs"
      description: "8位无符号单通道减法运算（饱和）"
    - name: "nppiSub_32f_C1R"
      description: "32位浮点单通道减法运算"
    - name: "nppiMul_8u_C1RSfs"
      description: "8位无符号单通道乘法运算（饱和）"
    - name: "nppiMul_32f_C1R"
      description: "32位浮点单通道乘法运算"
    - name: "nppiDiv_8u_C1RSfs"
      description: "8位无符号单通道除法运算（饱和）"
    - name: "nppiDiv_32f_C1R"
      description: "32位浮点单通道除法运算"
    - name: "nppiAddC_8u_C1RSfs"
      description: "8位无符号单通道常数加法"
    - name: "nppiAddC_32f_C1R"
      description: "32位浮点单通道常数加法"
    - name: "nppiAbs_8s_C1R"
      description: "8位有符号单通道绝对值"
    - name: "nppiAbs_32f_C1R"
      description: "32位浮点单通道绝对值"
    - name: "nppiAnd_8u_C1R"
      description: "8位无符号单通道按位与"
    - name: "nppiOr_8u_C1R"
      description: "8位无符号单通道按位或"
    - name: "nppiAndC_8u_C1R"
      description: "8位无符号单通道与常数按位与"
    - name: "nppiOrC_8u_C1R"
      description: "8位无符号单通道与常数按位或"

nppi_color_conversion_operations:
  description: "NPPI颜色空间转换"
  functions:
    - name: "nppiRGBToYUV_8u_C3R"
      description: "RGB到YUV颜色空间转换"
    - name: "nppiYUVToRGB_8u_C3R"
      description: "YUV到RGB颜色空间转换"
    - name: "nppiNV12ToRGB_8u_P2C3R"
      description: "NV12到RGB颜色转换"
    - name: "nppiNV12ToRGB_709CSC_8u_P2C3R"
      description: "NV12到RGB颜色转换（BT.709）"
    - name: "nppiNV12ToRGB_709HDTV_8u_P2C3R"
      description: "NV12到RGB颜色转换（BT.709 HDTV）"

nppi_data_exchange_operations:
  description: "NPPI数据交换操作"
  functions:
    - name: "nppiTranspose_8u_C1R"
      description: "8位无符号单通道矩阵转置"
    - name: "nppiTranspose_32f_C1R"
      description: "32位浮点单通道矩阵转置"
    - name: "nppiTranspose_16u_C1R"
      description: "16位无符号单通道矩阵转置"

nppi_filtering_functions:
  description: "NPPI滤波函数"
  functions:
    - name: "nppiFilterBox_8u_C1R"
      description: "8位无符号单通道盒式滤波"
    - name: "nppiFilterBox_32f_C1R"
      description: "32位浮点单通道盒式滤波"

nppi_geometry_transforms:
  description: "NPPI几何变换"
  functions:
    - name: "nppiMirror_8u_C1R"
      description: "8位无符号单通道镜像变换"
    - name: "nppiMirror_8u_C1IR"
      description: "8位无符号单通道原地镜像变换"

nppi_linear_transform_operations:
  description: "NPPI线性变换操作"
  functions:
    - name: "nppiMagnitude_32fc_32f_C1R"
      description: "32位复数到32位浮点幅值计算"
    - name: "nppiMagnitude_32f_C2R"
      description: "32位浮点双通道幅值计算"

nppi_support_functions:
